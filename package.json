    "dev": "concurrently \"pnpm next-dev\" \"pnpm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "prettier --write . && eslint --cache --fix .",
    "clean": "rm -rf node_modules .next next-env.d.ts"
