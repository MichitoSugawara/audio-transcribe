{
  "name": "audio-transcribe",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "i": "pnpm python:init",
    "install": "pnpm python:init",
    "python:init": "pip3 install -e whisperx && pip3 install -r requirements.txt",
    "fastapi-dev": "python3 -m uvicorn python.api:app --reload",
