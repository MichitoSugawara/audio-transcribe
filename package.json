    "@nextui-org/switch": "2.0.18",
    "@nextui-org/system": "2.0.7",
    "@nextui-org/theme": "2.1.6",
    "@react-aria/ssr": "^3.7.1",
    "@react-aria/visually-hidden": "^3.8.3",
    "@types/node": "20.5.7",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.1",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
